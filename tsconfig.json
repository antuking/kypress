{
    "compilerOptions": {
      "target": "es5",
      "lib": ["es5", "dom"],
      "types": ["cypress", "node"],
      "allowJs": true,
      "module": "CommonJS",
      "moduleResolution": "Node",
      "preserveValueImports": false,
      "outDir": "./dist/",
      "noImplicitAny": true
    },
    "include": ["cypress", "cypress/e2e/tests/*.ts", "cypress.config.ts"],
    "exclude": ["node_modules"]
}