{
  "name": "kypress-framework",
  "version": "1.0.0",
  "description": "kypress test automation framework",
  "scripts": {
    "open": "npx cypress open",
    "results-junit:delete": "rm -rf cypress/results/junit/* || true",
    "results-mocha:delete": "rm -rf cypress/results/cypress-mochawesome-reporter/.jsons/* || true",
    "report:pre": "npm run results-junit:delete",
    "test:registration": "npx cypress run --spec cypress/e2e/tests/registration.spec.cy.js"
  },
  "devDependencies": {
    "@cypress/grep": "^4.1.0",
    "@faker-js/faker": "^9.0.1",
    "cypress": "^13.14.2",
    "cypress-mochawesome-reporter": "^3.8.2",
    "cypress-multi-reporters": "^1.6.4",
    "eslint": "^8.48.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-chai-friendly": "^1.0.1",
    "eslint-plugin-cypress": "^3.5.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-n": "^16.0.2",
    "eslint-plugin-promise": "^6.1.1",
    "junit-report-merger": "^7.0.0",
    "mocha-junit-reporter": "^2.2.1"
  }
}
